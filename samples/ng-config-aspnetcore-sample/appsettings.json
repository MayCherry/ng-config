{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Spa": {
    "PreRenderEnabled": false,
    "PreRenderModule": "/dist/main",
    "PreRenderEnvironmentVariables": {
      "prod": "true",
      "aot": "true",
      "universal": "true"
    }
  },

  "Localization": {
    "DefaultLanguage": "en-US",
    "AvailableLanguages": [
      "en-US",
      "my-MM",
      "my-Qaaz-MM"
    ],
    "AutoDetectLanguage": true,
    "UseLocalizedRoutes": true
  }
}

