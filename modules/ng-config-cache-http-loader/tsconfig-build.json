{
  "extends": "../tsconfig-build.json",

  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": ".",
    "paths": {
      "@bizappframework/ng-config": [ "../../dist/packages/ng-config" ],
      "@bizappframework/ng-config/http-loader": [ "../../dist/packages/ng-config/http-loader" ]
    },
    "outDir": "../../dist/packages/ng-config-cache-http-loader"
  },
  "files": [
    "public_api.ts"
  ],
  "angularCompilerOptions": {
    "annotateForClosureCompiler": true,
    "strictMetadataEmit": true,
    "skipTemplateCodegen": true,
    "fullTemplateTypeCheck": true,
    "flatModuleOutFile": "index.js",
    "flatModuleId": "@bizappframework/ng-config-cache-http-loader"
  }
}
