{
  "extends": "../tsconfig-build.json",

  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": ".",
    "paths": {
      "@ngrx/store": [ "../../node_modules/@ngrx/store" ],
      "@bizappframework/ng-config": [ "../../dist/packages/ng-config" ]
    },
    "outDir": "../../dist/packages/ng-config-ngrx-store"
  },
  "files": [
    "public_api.ts"
  ],
  "angularCompilerOptions": {
    "annotateForClosureCompiler": true,
    "strictMetadataEmit": true,
    "flatModuleOutFile": "index.js",
    "flatModuleId": "@bizappframework/ng-config-ngrx-store"
  }
}
